{
  "rewrites": [
    {
      "source": "/sos/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/index.html"
    }
  ],
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "client/dist" }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ]
}
